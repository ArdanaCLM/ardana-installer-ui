{
    "name": "ardana-install-ui",
    "version": "4.0.0",
    "description": "Ardana Installer User Interface - Day Zero Experience",
    "scripts": {
        "devstart": "npm run inject && env HOME=$(pwd)/.. gulp",
        "e2e": "webdriver-manager update && gulp client-e2e",
        "e2e-headless": "test/runHeadlessE2e.sh",
        "e2e-model-tests": "test/prepareModelTests.sh && webdriver-manager update && gulp model-tests",
        "inject": "gulp index",
        "lint": "gulp lint",
        "postinstall": "bower install",
        "prestart": "npm run inject",
        "production": "gulp production && npm run sass",
        "sass": "node-sass client/app/app.scss client/app/app.css",
        "start": "node server/server.js",
        "test": "gulp client-karma",
        "apiTest": "jasmine-node server/tests"
    },
    "dependencies": {
        "csurf": "1.8.3",
        "express": "4.13.4",
        "express-session": "1.11.3",
        "helmet": "0.10.0",
        "ardana-service": "git://git.suse.provo.cloud/ardana/ardana-service#master",
        "js-yaml": "3.3.1",
        "node-uuid": "1.4.3"
    },
    "devDependencies": {
        "bower": "^1.4.1",
        "browser-sync": "^2.8.2",
        "del": "^2.2.0",
        "gulp": "^3.9.0",
        "gulp-angular-filesort": "^1.1.1",
        "gulp-inject": "^1.5.0",
        "gulp-nodemon": "^2.0.3",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^2.0.4",
        "gulp-gjslint": "0.1.5",
        "gulp-istanbul": "0.10.3",
        "gulp-tap": "0.1.3",
        "istanbul": "0.4.2",
        "istanbul-combine": "0.3.0",
        "jasmine-core": "^2.3.4",
        "jasmine-node": "^1.14.5",
        "jasmine-spec-reporter": "2.4.0",
        "karma": "^0.13.3",
        "karma-cli": "^0.1.0",
        "karma-coverage": "^0.4.2",
        "karma-jasmine": "^0.3.6",
        "karma-ng-html2js-preprocessor": "^0.1.2",
        "karma-phantomjs-launcher": "^0.2.0",
        "lodash": "^3.10.1",
        "node-sass": "^3.2.0",
        "needle" : "1.0.0",
        "phantomjs": "^1.9.17",
        "protractor": "^2.2.0",
        "proxyquire": "^1.7.2",
        "run-sequence": "1.1.4",
        "supertest": "^1.1.0",
        "vinyl-fs": "2.4.2",
        "wiredep": "^2.2.2",
        "wrench": "^1.5.8"
    }
}
